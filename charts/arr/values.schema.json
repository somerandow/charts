{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
        "affinity": {
            "type": "object"
        },
        "image": {
            "type": "object",
            "properties": {
                "pullPolicy": {
                    "type": "string"
                },
                "repository": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "ingress": {
            "type": "object",
            "properties": {
                "api": {
                    "type": "object",
                    "properties": {
                        "annotations": {
                            "type": "object"
                        },
                        "className": {
                            "type": "string"
                        },
                        "enabled": {
                            "type": "boolean"
                        },
                        "forwardAuth": {
                            "type": "object",
                            "properties": {
                                "allowedGroups": {
                                    "type": "array"
                                },
                                "enabled": {
                                    "type": "boolean"
                                },
                                "endpoint": {
                                    "type": "string"
                                }
                            }
                        },
                        "host": {
                            "type": "string"
                        },
                        "path": {
                            "type": "string"
                        },
                        "pathType": {
                            "type": "string"
                        },
                        "tls": {
                            "type": "boolean"
                        }
                    }
                },
                "main": {
                    "type": "object",
                    "properties": {
                        "annotations": {
                            "type": "object"
                        },
                        "className": {
                            "type": "string"
                        },
                        "enabled": {
                            "type": "boolean"
                        },
                        "forwardAuth": {
                            "type": "object",
                            "properties": {
                                "allowedGroups": {
                                    "type": "array"
                                },
                                "enabled": {
                                    "type": "boolean"
                                },
                                "endpoint": {
                                    "type": "string"
                                }
                            }
                        },
                        "host": {
                            "type": "string"
                        },
                        "path": {
                            "type": "string"
                        },
                        "pathType": {
                            "type": "string"
                        },
                        "tls": {
                            "type": "boolean"
                        }
                    }
                }
            }
        },
        "nameOverride": {
            "type": "string"
        },
        "nodeSelector": {
            "type": "object"
        },
        "persistence": {
            "type": "object",
            "properties": {
                "config": {
                    "type": "object",
                    "properties": {
                        "accessModes": {
                            "type": "array"
                        },
                        "annotations": {
                            "type": "object"
                        },
                        "className": {
                            "type": "string"
                        },
                        "enabled": {
                            "type": "boolean"
                        },
                        "size": {
                            "type": "string"
                        }
                    }
                },
                "media": {
                    "type": "object",
                    "properties": {
                        "accessModes": {
                            "type": "array"
                        },
                        "annotations": {
                            "type": "object"
                        },
                        "className": {
                            "type": "string"
                        },
                        "enabled": {
                            "type": "boolean"
                        },
                        "existingClaim": {
                            "type": "string"
                        },
                        "size": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "podAnnotations": {
            "type": "object"
        },
        "podSecurityContext": {
            "type": "object"
        },
        "probes": {
            "type": "object",
            "properties": {
                "livenessCmd": {
                    "type": "array"
                }
            }
        },
        "resources": {
            "type": "object"
        },
        "securityContext": {
            "type": "object"
        },
        "service": {
            "type": "object",
            "required": [
                "port"
            ],
            "properties": {
                "port": {
                    "type": "integer",
                    "minimum": 1024
                }
            }
        },
        "serviceAccount": {
            "type": "object",
            "properties": {
                "create": {
                    "type": "boolean"
                }
            }
        },
        "tolerations": {
            "type": "array"
        }
    }
}
